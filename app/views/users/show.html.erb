<div class="row">
	<div class="col-md-12">
		
		<div class="form-group">
			<label class="col-sm-2 control-label">Email:</label>
			<div class="col-sm-10">
				<p class="form-control-static"><%=h @user.email %></p>
			</div>
		</div>

		<div class="form-group">
			<label class="col-sm-2 control-label">Login count:</label>
			<div class="col-sm-10">
				<p class="form-control-static"><%=h @user.login_count %></p>
			</div>
		</div>

		<div class="form-group">
			<label class="col-sm-2 control-label">Last request at:</label>
			<div class="col-sm-10">
				<p class="form-control-static"><%=h @user.last_request_at %></p>
			</div>
		</div>

		<div class="form-group">
			<label class="col-sm-2 control-label">Last login at:</label>
			<div class="col-sm-10">
				<p class="form-control-static"><%=h @user.last_login_at %></p>
			</div>
		</div>

		<div class="form-group">
			<label class="col-sm-2 control-label">Current login at:</label>
			<div class="col-sm-10">
				<p class="form-control-static"><%=h @user.current_login_at %></p>
			</div>
		</div>

		<div class="form-group">
			<label class="col-sm-2 control-label">Last login ip:</label>
			<div class="col-sm-10">
				<p class="form-control-static"><%=h @user.last_login_ip %></p>
			</div>
		</div>

		<div class="form-group">
			<label class="col-sm-2 control-label">Current login ip:</label>
			<div class="col-sm-10">
				<p class="form-control-static"><%=h @user.current_login_ip %></p>
			</div>
		</div>
		<%= link_to 'Edit Account', edit_account_path %>
	
	</div>
</div>
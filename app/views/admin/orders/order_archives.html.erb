<table>
  <thead>
  <tr>
    <th>Id</th>
    <th>Status</th>
    <th>Data płatności</th>
    <th>Data złożenia</th>
  </tr>
  </thead>

  <tbody>
  <% @orders.each do |order| %>
      <tr>
        <td><%= order.id %></td>
        <td><%= order.status %></td>
        <td><%= order.payment %></td>
        <td><%= order.created_at %></td>
      </tr>
  <% end %>
  </tbody>
</table>
